

<h1>Bazesprods</h1>

<table>
  <thead>
    <tr>
      <th>Prodnos</th>
      <th>Nutrition1</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @bazesprods.each do |bazesprod| %>
      <tr>
        <td><%= bazesprod.prodnos %></td>
        <td><%= bazesprod.nutrition1 %></td>
          <% if current_user.admin %>
            <td><%= link_to 'Show', bazesprod %></td>
            <td><%= link_to 'Edit', edit_bazesprod_path(bazesprod) %></td>
            <td><%= link_to 'Destroy', bazesprod, method: :delete, data: { confirm: 'Are you sure?' } %></td>
          <% end %>
      </tr>
    <% end %>
  </tbody>
</table>

<br>
<% if current_user.admin %>
  <%= link_to 'New Bazesprod', new_bazesprod_path %>
<% end %>