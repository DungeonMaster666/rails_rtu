

<h1>Bazesprods</h1>

<table>
  <thead>
    <tr>
      <th>Produkts</th>
      <th>Olbaltumvielas</th>
      <th>Tauki</th>
      <th>OgÄ¼h.</th>
      <th>Kcal</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @bazesprods.each do |bazesprod| %>
      <tr>
        <td><%= bazesprod.prodnos %></td>
        <td><%= bazesprod.olb %></td>
        <td><%= bazesprod.tauki %></td>
        <td><%= bazesprod.oglh %></td>
        <td><%= bazesprod.kcal %></td>
        <td><%= link_to 'Show', bazesprod %></td>
        <% if current_user && current_user.admin? %>
            <td><%= link_to 'Edit', edit_bazesprod_path(bazesprod) %></td>
            <td><%= link_to 'Destroy', bazesprod, method: :delete, data: { confirm: 'Are you sure?' } %></td>
          <% end %>
      </tr>
    <% end %>
  </tbody>
</table>

<br>



<div class="container-fluid bg-light">
  <div class="col-md-12 text-center">
    <div class="row">
      <div class="col">
        <% if current_user && current_user.admin? %>
          <%= link_to 'New Bazesprod', new_bazesprod_path %>
        <% end %>
      </div>
      <div class="col">
        <div class="container pagination justify-content-center" >
          <%= paginate @bazesprods %>
        </div>
      </div>
      <div class="col">
      </div>
    </div>
  </div>
</div>

<br/><br/>