

<h1>Velprods</h1>

<table>
  <thead>
    <tr>
      <th>User</th>
      <th>Prod nos</th>
      <th>Nutrition1</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @velprods.each do |velprod| %>
      <% if current_user == velprod.user %>
        <tr>
          <td><%= velprod.user_id %></td>
          <td><%= velprod.prod_nos %></td>
          <td><%= velprod.nutrition1 %></td>
          <td><%= link_to 'Edit', edit_velprod_path(velprod) %></td>
          <td><%= link_to 'Destroy', velprod, method: :delete, data: { confirm: 'Are you sure?' } %></td>
        </tr>
      <% end %>
    <% end %>
  </tbody>
</table>

<br/><br/>


<select class="form-select" aria-label="Default select example">
  <%= options_for_select(@bazesprodsaraksts)  %>
</select>

<br>

<%= link_to 'New Velprod', new_velprod_path %>
